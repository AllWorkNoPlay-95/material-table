{
  "name": "material-table",
  "version": "2.0.5",
  "description": "Datatable for React based on https://material-ui.com/api/table/ with additional features",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "babel": {
    "extends": "./configs/.babelrc"
  },
  "scripts": {
    "prepare": "npm run build",
    "start": "webpack-dev-server --config ./demo/webpack.config.js --mode development",
    "build": "babel src --out-dir dist",
    "lint": "npm run eslint && npm run tsc",
    "eslint": "eslint src/** -c ./configs/.eslintrc",
    "tsc": "tsc --noEmit --lib es6,dom --skipLibCheck types/index.d.ts",
    "lint:fix": "eslint src/** --fix",
    "prettify": "prettier --write **/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/databucket/material-table.git"
  },
  "keywords": [
    "react",
    "material-ui",
    "material",
    "datatable",
    "table"
  ],
  "author": "Mehmet Baran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/databucket/material-table/issues"
  },
  "homepage": "https://github.com/databucket/material-table#readme",
  "devDependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "7.23.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-transform-runtime": "7.22.15",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-react": "7.22.15",
    "@mui/material": "5.14.11",
    "@mui/lab": "5.0.0-alpha.146",
    "@mui/styles": "5.14.11",
    "@mui/x-date-pickers": "6.19.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.3.0",
    "babel-polyfill": "6.26.0",
    "buble": "0.20.0",
    "eslint": "8.50.0",
    "eslint-config-defaults": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-standard": "5.0.0",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "pretty-quick": "3.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.1",
    "typescript": "5.2.2",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "dependencies": {
    "@date-io/date-fns": "3.0.0",
    "@emotion/styled": "11.11.0",
    "classnames": "2.3.2",
    "date-fns": "^3.2.0",
    "debounce": "1.2.1",
    "fast-deep-equal": "3.1.3",
    "filefy": "0.1.11",
    "jspdf": "2.5.1",
    "jspdf-autotable": "3.6.0",
    "prop-types": "15.8.1",
    "react-beautiful-dnd": "13.1.1",
    "react-double-scrollbar": "0.0.15"
  },
  "peerDependencies": {
    "@date-io/core": "3.0.0",
    "@mui/material": "~5.14.11",
    "@mui/lab": "~5.0.*",
    "@mui/styles": "~5.14.11",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}
